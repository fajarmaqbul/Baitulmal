{
    "version": 2,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.1"
        },
        "api/health.php": {
            "runtime": "vercel-php@0.7.1"
        },
        "api/debug.php": {
            "runtime": "vercel-php@0.7.1"
        },
        "api/db_check.php": {
            "runtime": "vercel-php@0.7.1"
        },
        "api/test.php": {
            "runtime": "vercel-php@0.7.1"
        },
        "api/env_check.php": {
            "runtime": "vercel-php@0.7.1"
        }
    },
    "routes": [
        {
            "src": "/test-php",
            "dest": "api/test.php"
        },
        {
            "src": "/health",
            "dest": "api/health.php"
        },
        {
            "src": "/debug",
            "dest": "api/debug.php"
        },
        {
            "src": "/db-check",
            "dest": "api/db_check.php"
        },
        {
            "src": "/env-check",
            "dest": "api/env_check.php"
        },
        {
            "src": "/(.*)",
            "dest": "api/index.php"
        }
    ],
    "env": {
        "APP_NAME": "Baitulmall Masjid Kandri",
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "LOG_CHANNEL": "stderr",
        "DB_CONNECTION": "pgsql",
        "VERCEL": "1"
    }
}