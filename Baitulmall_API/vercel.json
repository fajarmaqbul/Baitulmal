{
    "version": 2,
    "name": "baitulmal-api",
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "LOG_CHANNEL": "stderr",
        "DB_CONNECTION": "pgsql"
    }
}